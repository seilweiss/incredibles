#ifndef _MSL_CMATH_H
#define _MSL_CMATH_H

#include "math.h"

namespace std
{
    float floorf(float);

    inline float sinf(float x)
    {
        return sin(x);
    }

    inline float cosf(float x)
    {
        return cos(x);
    }

    inline float tanf(float x)
    {
        return tan(x);
    }
} // namespace std

#endif